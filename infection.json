{
    "timeout": 2,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "infection-log.txt"
    },
    "minMsi": 100,
    "minCoveredMsi": 100,
    "mutators": {
        "global-ignoreSourceCodeByRegex": [
            "assert\\(.+\\);"
        ],
        "@default": true
    }
}
